{
  "compilerOptions": {
    "target": "ES2020",
  "module": "Node16",
  "moduleResolution": "Node16",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "ignoreDeprecations": "6.0",
  "include": [".astro/types.d.ts", "src", "src/**/*", "**/*"],
  "exclude": ["dist", "node_modules"]
}
